<% @team_members.each do |member|%>
  <%= form_for member, :html => { :multipart => true } do |f| %>
    <%= f.text_field :name %>
    <%= f.text_field :title %>
    <%= f.text_field :photo_url %>
    <%= f.text_area :bio, size: "60x12" %>
    <%= f.submit %>
    <%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %>
  <% end %>
<% end %>