<%= form_for @event, :html => { :multipart => true } do |f| %>
    <%= f.text_field :title %>
    <%= f.text_field :frequency %>
    <%= f.text_field :description %>
    <%= f.text_field :album_link %>
    <%= f.text_field :frequency %>
    <%= date_select('event', :date_of_event) %>
  <%= f.submit %>
<% end %>
